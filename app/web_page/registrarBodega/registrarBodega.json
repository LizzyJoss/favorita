        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.proveedor && $data.producto && $data.bodega ){",
        "\treturn {",
        "\t\t//map proveedor variable to expected task contract input",
        "\t\tproveedorInput: {",
        "\t\t\tnombre: $data.proveedor.nombre !== undefined ? $data.proveedor.nombre : null",
        "\t\t},",
        "\t\t//map producto variable to expected task contract input",
        "\t\tproductoInput: {",
        "\t\t\tnombreProducto: $data.producto.nombreProducto !== undefined ? $data.producto.nombreProducto : null,",
        "\t\t\tprecio: $data.producto.precio !== undefined ? $data.producto.precio : null,",
        "\t\t\tcantidad: $data.producto.cantidad !== undefined ? $data.producto.cantidad : null",
        "\t\t},",
        "\t\t//map bodega variable to expected task contract input",
        "\t\tbodegaInput: {",
        "\t\t\tid: $data.bodega.id !== undefined ? $data.bodega.id : null,",
        "\t\t\tnombreBodega: $data.bodega.nombreBodega !== undefined ? $data.bodega.nombreBodega : null,",
        "\t\t\tdireccionBodega: $data.bodega.direccionBodega !== undefined ? $data.bodega.direccionBodega : null",
        "\t\t}",
        "\t}",
        "}"
      ],
