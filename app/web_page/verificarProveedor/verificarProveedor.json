      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.proveedor ){",
        "\treturn {",
        "\t\t//map proveedor variable to expected task contract input",
        "\t\tproveedorInput: {",
        "\t\t\tid: $data.proveedor.id !== undefined ? $data.proveedor.id : null,",
        "\t\t\tnombre: $data.proveedor.nombre !== undefined ? $data.proveedor.nombre : null,",
        "\t\t\tdireccion: $data.proveedor.direccion !== undefined ? $data.proveedor.direccion : null,",
        "\t\t\ttelefono: $data.proveedor.telefono !== undefined ? $data.proveedor.telefono : null,",
        "\t\t\tcorreoElectronico: $data.proveedor.correoElectronico !== undefined ? $data.proveedor.correoElectronico : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
